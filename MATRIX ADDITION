#include <stdio.h>
void main()
{
     int a[3][3];
     int b[3][3];
     int c[3][3];
     printf("enter elements of first matrix:");
     for(int i=0;i<=2;i++)
     {
         for(int j=0;j<=2;j++)
         {
             scanf("%d",&a[i][j]);
         }
     }
     printf("enter elements of second matrix:");
     for(int i=0;i<=2;i++)
     {
         for(int j=0;j<=2;j++)
         {
             scanf("%d",&b[i][j]);
         }
     }
     printf("FIRST MATRIX\n");
     for(int i=0;i<=2;i++)
     {
         for(int j=0;j<=2;j++)
         {
             printf(" %d",a[i][j]);
         }
         printf("\n");
     }
     printf("SECOND MATRIX\n");
     for(int i=0;i<=2;i++)
     {
         for(int j=0;j<=2;j++)
         {
             printf(" %d",b[i][j]);
         }
         printf("\n");
     }
     for(int i=0;i<=2;i++)
     {
         for(int j=0;j<=2;j++)
         {
            c[i][j]=a[i][j]+b[i][j];
         }
     }
     printf("the sum is \n");
     for(int i=0;i<=2;i++)
     {
         for(int j=0;j<=2;j++)
         {
             
             printf(" %d",c[i][j]);
         }
         printf("\n");
     }
}
