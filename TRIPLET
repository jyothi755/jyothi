#include<stdio.h>
void main()
{
    int i,j,k,n;
    printf("enter a number");
    scanf("%d",&n);
    for(i=5;i<=n;i++)
    {
        for(j=4;j<i;j++)
        {
            for(k=3;k<j;k++)
            {
                if(j*j+k*k==i*i)
                printf("\n%d %d %d",k,j,i);
            }
        }
    }
}
