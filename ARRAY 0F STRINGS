#include<stdio.h>
#include<string.h>
void main()
{
   char a[10][10];
   int n,i;
   printf("enter no of words");
   scanf("%d",&n);
   printf("enter the names");
   for(i=0;i<n;i++)
   {
       scanf("%s",a[i]);
   }
   printf("the names are:");
   for(i=0;i<n;i++)
   {
       printf("\n%d.%s",i+1,a[i]);
   }
}
