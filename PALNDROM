#include<stdio.h>
#include<string.h>
#include<conio.h>
void main()
{
    char a[20];
    int l=strlen(a)-1;
    printf("Enter a string:");
    gets(a);
    for(int i=0,j=l;i<=j;i++,j--)
    {
       if(a[i]!=a[j])
       {
          printf("It is not palindrome");
          return;
       }
    }
    printf("It is palindrome");       
}
